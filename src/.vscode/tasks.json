{
    "version": "2.0.0",
    "type": "shell",
    "problemMatcher": "$msCompile",
    "tasks": [
        {
            "label": "tmbuild",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "windows": {
                "command": "%TM_SDK_DIR%//bin//tmbuild.exe"
            },
            "problemMatcher": [
                "$msCompile"
            ]
        },
        {
            "label": "docgen",
            "group": "build",
            "windows": {
                "command": "%TM_SDK_DIR%//bin//docgen.exe"
            },
            "args": [
                "-e",
                "dinosaur_simulate.c"
            ],
        },
    ]
}